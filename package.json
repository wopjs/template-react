{
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "eslint && prettier --check .",
    "lint:fix": "eslint --fix && prettier --write .",
    "ts-check": "tsc --noEmit",
    "ts-watch": "tsc --noEmit -w",
    "dev": "vite --host --open",
    "build": "tsc -b && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "@wopjs/eslint-config": "^0.1.13",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "sass": "^1.85.1",
    "typescript": "^5.8.2",
    "unocss": "^66.0.0",
    "vite": "^6.2.1"
  },
  "dependencies": {
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
